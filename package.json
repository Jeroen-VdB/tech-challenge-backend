{
  "name": "tech-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "UNLICENSED",
  "private": "true",
  "scripts": {
    "clean": "rm dist",
    "build": "tsc",
    "prestart": "npm run db:init && npm run db:latest",
    "start": "NODE_PATH=./dist node --require ./dist/instrumentation.js ./dist/index.js",
    "local": "node --require ts-node/register --require ./src/instrumentation.ts --inspect=7000 ./src/index.ts",
    "dev": "nodemon --exec \"node --require ts-node/register --require ./src/instrumentation.ts\" --inspect=7000 ./src/index.ts",
    "db:init": "node dist/db/create-database.js",
    "db:latest": "npx knex migrate:latest --knexfile dist/knexfile.js",
    "lint:tdd": "nodemon --watch ./src --ext ts  --exec npm run lint",
    "lint": "eslint . --ext .ts",
    "test:tdd": "nodemon --delay 0.01 --inspect=7001 --watch ./src --ext ts,json --exec npm test",
    "test": "lab ./src --pattern \"\\.spec\""
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^6.0.1",
    "@google-cloud/opentelemetry-cloud-monitoring-exporter": "^0.20.0",
    "@google-cloud/opentelemetry-cloud-trace-exporter": "^2.3.0",
    "@hapi/boom": "~10.0.1",
    "@hapi/hapi": "~21.4.0",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation-http": "^0.54.0",
    "@opentelemetry/instrumentation-knex": "^0.42.0",
    "@opentelemetry/instrumentation-winston": "^0.42.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "hapi-swagger": "^17.3.2",
    "joi": "~17.13.3",
    "knex": "~3.1.0",
    "mysql": "~2.18.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.2",
    "@hapi/lab": "^24.0.0",
    "@types/hapi__hapi": "^20.0.0",
    "@types/sinon": "^9.0.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "lab-transform-typescript": "^3.0.1",
    "nodemon": "^2.0.4",
    "sinon": "^9.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.9.5"
  }
}
